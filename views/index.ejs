<!DOCTYPE html>
<html>
<head>
  <title>parsing JSON</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
</head>
<body>
    <table class="table">
      <thead>
      <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Created At</th>
        <th>Updated At</th>
        <th>User ID</th>
      </tr>
      </thead>
      <tbody>
      <% for (var i = 0; i < users.length; i++) { %>
      <tr>
        <td> <%= users[i].username %> </td>
        <td> <%= users[i].firstname %> </td>
        <td> <%= users[i].lastname %> </td>
        <td> <%= users[i].age %> </td>
        <td> <%= users[i].created_at %> </td>
        <td> <%= users[i].updated_at %> </td>
        <td> <%= users[i].id %> </td>
        <td><a href="/users/<%= users[i].id %>/edit">Edit User</a></td>
        <td><a href="/users/<%= users[i].id %>/delete">Delete User</a></td>
      </tr>
      <% } %>
    </tbody>
    </table>
</body>
</html>
